{
    "plugins": [],
    "extends": ["plugin:vue/base"],
    "rules": {
      "indent": ["error", 2],
      "quotes": ["error", "single"]
    },
    "env": {
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "ignorePatterns": ["node_modules/"]
}